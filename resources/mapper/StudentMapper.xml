<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "mybatis-3-mapper.dtd">

<mapper namespace="com.hbs.dao.StudentDao">

	<select id="selectAll" resultType="com.hbs.entity.Student">
		SELECT * FROM student
	</select>
	
	<insert id="add" parameterType="com.hbs.entity.Student">
		insert into student(name,age,sex,clas,addr) values(#{name},#{age},#{sex},#{clas},#{addr})
	</insert>
	
    
	
	
</mapper>